/**app.wxss**/
.clearfix::after {
  display: block;
  content: "";
  width: 0;
  height: 0;
  clear: both;
}
page{
  width: 100%;
  height:100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/*kyc-common  */
.kyc-bg {
  width:100%;
  height:100%;
  background: #2e3347 url('http://anan.teegoo.com/static/media/kycBg.7df9a2e2.png') no-repeat content-box;
  background-size: cover;
  position: relative;
}
.kyc-anan-wrap {
  display: block;
  padding:40px 0 0;
}
.kyc-anan {
  display: block;
  width:80px;
  height: 80px;
  margin:0 auto
}
.qu-title {
  display: flex;
  flex-direction: row;
  padding:20px 20px;
  font-size:20px;
  color:#fff;
  align-items: center;
}
.qu-title-list {
  font-size:14px;
  padding-right:8px;
  color:#797e9c;
}
.qu-title-number {
 font-size:24px;
 color:#fff;
}
.progress {
  display: block;
  width:100%;
  height:4px;
  background: rgba(255, 255, 255, 0.2);
  position:absolute;
  top:0;
  left:0;
  right:0;
}
.progress-i {
 display: block;
 position: absolute;
 top:0;
 left:0;
 background: #fb8801;
 height:4px;
}
.bt-next {
  display:flex;
  position:absolute;
  width:100%;
  left:0;
  bottom:12%;
  align-items: center;
  text-align: center;
  font-size:16px;
  color:#fff;
  justify-content: center;
}
.next-arr {
  display: block;
  width:18px;
  height: 17px;
  margin-left:6px;
}
.bt-grey {
 color: rgba(154, 157, 175, 0.7);
 font-size:12px;
 display: block;
 width:100%;
 padding:10px 0;
 text-align:center;
 position: absolute;
 bottom:2%;
 left:0;
}
.pt {
  display: flex;
  flex-direction: row;
  position:absolute;
  top:20px;
  left:20px;
  align-content: center;
  width:80%;
  padding:10px 0;
}
.prev-arr {
  display: inline-block;
  width:18px;
  height:17px;
  margin-right:6px;
  vertical-align: middle;
}
.prev-t {
  display:block;
  font-size: 16px;
  color: #fff; 
}


.fAge {
  padding:0 20px;
  width:100%;
  box-sizing:border-box;
}
.fAge::after{
  display: block;
  content: "";
  width: 0;
  height: 0;
  clear: both;
}
.ageItem {
  display: block;
  width: 46%;
  height: 48px;
  color: #fff;
  float: left;
  border-radius: 100px;
  margin-bottom: 10px;
  font-size: 14px;
  line-height: 48px;
  position: relative;
}
.ageInput {
  display: inline-block;
  height: 48px;
  line-height: 48px;
  border: 0;
  width: 100%;
  font-size: 18px;
  background: transparent;
  color: #fff;
  vertical-align: middle;
  text-align: right;
  padding-right: 4px;
  background: #757a97;
  border-radius: 50px;
  padding: 10px 30px 10px 90px;
  box-sizing: border-box;
}
.fAge .ageItem:nth-child(2n) {
  float: right;
}
.tSpan {
  color: rgba(255, 255, 255, 0.6);
  display: inline-block;
  text-align: center;
  position: absolute;
  width: 90px;
  left: 0;
}
.ageS {
  color: rgba(255, 255, 255, 0.6);
  display: inline-block;
  width: 30px;
  text-align: center;
  position: absolute;
  right: 0;
}
